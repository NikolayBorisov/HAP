{
    "python.pythonPath": "/Applications/Xcode.app/Contents/Developer/usr/bin/python3",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.pyc": true,
        "**/__pycache__": true
    },
    "python.formatting.autopep8Args": [
        "--max-line-length=140"
    ]
}